{
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "php/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "php/tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}